<h1>Users#edit</h1>

<P><%= render "validation_errors", object:  @user %><P>

<%= form_for @user, :url => users_admin_path do |f| %>
	<p>
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	</p>

	<p>
		<%= f.label :role %><br>
		<%= f.text_field :role %>
	</p>

	<P><%= f.submit %></P>
<% end %>
