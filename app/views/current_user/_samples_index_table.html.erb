<p><%= link_to "All Samples", samples_path, class: "pull-xs-right" %></p>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>Project Name</th>
			<th>Quantity</th>
			<th>Arrive Date</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
		<% object.last(2).each do |sample| %>
			<tr>
				<td><%= sample.project.project_name %></td>
				<td><%= sample.quantity%></td>
				<td><%= sample.receive_date.strftime("%m-%d-%y") %></td>
				<td><%= sample.description%></td>
			</tr>
		<% end %>
	</tbody>
</table>

