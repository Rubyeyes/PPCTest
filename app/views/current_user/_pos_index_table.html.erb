<p><%= link_to "All PO", pos_path, class: "pull-xs-right"%></p>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>PO Number</th>
			<th>Created at</th>
		</tr>
	</thead>
	<tbody>
		<% object.last(2).each do |po| %>
			<tr>
				<td><%= po.po_number %></td>
				<td><%= po.date.strftime("%b %d. %y") %></td>
			</tr>
		<% end %>
	</tbody>
</table>

