<div class="container-home">
  <div class="col-xs-12 col-md-12 col-no-padding">
    <div class="home-video" >
      <iframe frameborder="0" height="100%" width="100%" src="https://www.youtube.com/embed/68P4dP1Pqug?autoplay=1&loop=1&rel=0&amp;controls=0&amp;showinfo=0&list=PLLIoHa3Ztev54UOnOudZj48iZpl54loUM&autohide=1" allowfullscreen></iframe>
    </div><!-- 
    <img class="img-responsive img-thumbnail project-image" src="/home_images/upper.jpg"> style="position: fixed; z-index: 0; width: 100%; height: 710px"-->
    <div class="jumbotron jumbotron-home" style="position:relative;height:710px;padding-top:25%">
    </div>
  </div>
  <div class="section">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/upper.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/concept.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/mark.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/mount.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/projects.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/reflex sight.jpg">
    <img class="img-responsive img-thumbnail project-image" src="/home_images/scope.jpg">
    <div hidden id="login-panel" class="panel panel-default panel-body panel-body-login">
        <h2>Sign in</h2>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="form-inline">
            <%= f.label :email, class: "form-label" %><br />
            <%= f.email_field :email, autofocus: true, class: "form-control"  %>
          </div>

          <div class="form-inline">
            <%= f.label :password, class: "form-label"  %><br />
            <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
          </div>

          <% if devise_mapping.rememberable? -%>
            <div class="form-inline">
              <%= f.check_box :remember_me, class: "form-control" %>
              <%= f.label :remember_me, class: "form-label" %>
            </div>
          <% end -%>

          <div class="actions">
            <p><%= f.submit "Sign in", class: "btn btn-default" %></p>
          </div>
        <% end %>
        <%= render "devise/shared/links" %>
    </div>
  </div>
</div>